{
	"name": "inline.px",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "npm run generate:types && npm run generate:docs && vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"generate:types": "tsx scripts/generate-tool-types.ts",
		"generate:docs": "tsx scripts/generate-tool-docs.ts"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/kit": "^2.48.5",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@types/node": "^22.19.1",
		"svelte": "^5.43.8",
		"svelte-check": "^4.3.4",
		"tsx": "^4.21.0",
		"typescript": "^5.9.3",
		"vite": "^7.2.2"
	},
	"dependencies": {
		"@lucide/svelte": "^0.555.0"
	}
}
